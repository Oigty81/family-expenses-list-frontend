{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": [
                "src/assets/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@lang/*": [
                "src/lang/*"
            ],
            "@layouts/*": [
                "src/layouts/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@router/*": [
                "src/router/*"
            ],
            "@stores/*": [
                "src/stores/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@utilities/*": [
                "src/utilities/*"
            ],
            "@/*": [
                "src/*"
            ]
        }
    }
}